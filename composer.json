{
    "name": "etna/lib-utils",
    "description": "Symfony 4 bundle to provide utils functions",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume DUBOST",
            "email": "guillaume@etna.io"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^4.1",
        "symfony/flex": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "ETNA\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "TestApp/app/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://blu-composer.herokuapp.com"
        }
    ],
    "require-dev": {
        "behat/behat": "^3.4",
        "sensio/framework-extra-bundle": "^5.1",
        "phpunit/php-code-coverage": "^6.0",
        "phpunit/phpcov": "^5.0",
        "behat/symfony2-extension": "^2.1",
        "phploc/phploc": "^4.0",
        "squizlabs/php_codesniffer": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "sebastian/phpcpd": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phing/phing": "^2.16",
        "php-coveralls/php-coveralls": "^2.1",
        "scrutinizer/ocular": "^1.5",
        "etna/composer-behat-utils": "^v3.0.0-alpha.1",
        "phpstan/phpstan": "^0.10.8",
        "povils/phpmnd": "^2.0",
        "phpmetrics/phpmetrics": "^2.4",
        "symfony/console": "^4.1",
        "symfony/translation": " 4.3.8",
        "etna/json-service-provider": "^3.0",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "etna/sprinter-service-provider": "^3.0"
    },
    "scripts": {
        "test": "behat",
        "behat": "behat",
        "phing": "phing",
        "coveralls": "php-coveralls",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
